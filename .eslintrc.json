{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "indent": [
      "error",
      2
    ]
  },
  "overrides": [
    {
      "files": [
        "**/*.js"
      ],
      "extends": "eslint:recommended"
    }
  ]
}